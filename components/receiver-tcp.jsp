<div class="row justify-content-center mt-2" id="receiver-tcp-page">
    <div class="col-md-11 config-box row">
        <div class="input-group mb-2 col-md-12 d-flex flex-column" id="properties-name">
            <label for="receiver-tcp-propname">Properties Name</label>
            <input type="text" class="input-field" id="receiver-tcp-propname">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-tcp-port">Port</label>
            <input type="number" class="input-field" id="receiver-tcp-port">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-tcp-timeout">Timeout</label>
            <input type="number" class="input-field" id="receiver-tcp-timeout">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-tcp-ip">IP</label>
            <input type="text" class="input-field" id="receiver-tcp-ip">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-tcp-timewait">Timewait</label>
            <input type="number" class="input-field" id="receiver-tcp-timewait">
        </div>
        <div class="input-group form-check mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-tcp-keep_open">KeepOpen</label>
            <input class="form-check-input" type="checkbox" id="receiver-tcp-keep_open" checked>
        </div>
    </div>
</div>