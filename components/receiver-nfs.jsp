<div class="row justify-content-center mt-2" id="receiver-nfs-page">
    <div class="col-md-11 config-box row">
        <div class="input-group mb-2 col-md-12 d-flex flex-column" id="properties-name">
            <label for="receiver-nfs-propname">Properties Name</label>
            <input type="text" class="input-field" id="receiver-nfs-propname">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-nfs-path">Path</label>
            <input type="text" class="input-field" id="receiver-nfs-path">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-nfs-polling">Polling (second)</label>
            <input type="number" class="input-field" id="receiver-nfs-polling">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-nfs-retry">Retry</label>
            <input type="number" class="input-field" id="receiver-nfs-retry" value="3">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label for="receiver-nfs-file_name">Filename</label>
            <input type="text" class="input-field" id="receiver-nfs-file_name">
        </div>
    </div>
</div>