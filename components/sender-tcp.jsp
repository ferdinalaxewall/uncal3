<div class="row justify-content-center mt-2" id="sender-tcp-page">
    <div class="col-md-11 config-box row">
        <div class="input-group mb-2 col-md-12 d-flex flex-column" id="properties-name">
            <label class="mb-0" for="sender-tcp-propname">Properties Name</label>
            <input type="text" class="input-field" id="sender-tcp-propname">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label class="mb-0" for="sender-tcp-port">Port</label>
            <input type="number" class="input-field" id="sender-tcp-port">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label class="mb-0" for="sender-tcp-name">Name</label>
            <input type="text" class="input-field" id="sender-tcp-name">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label class="mb-0" for="sender-tcp-thread">Thread</label>
            <input type="number" class="input-field" id="sender-tcp-thread">
        </div>
        <div class="input-group mb-2 col-md-12 d-flex flex-column">
            <label class="mb-0" for="sender-tcp-wait">Wait</label>
            <input type="number" class="input-field" id="sender-tcp-wait">
        </div>
        <div class="input-group form-check mb-2 col-md-12 d-flex flex-column">
            <label class="mb-0" for="sender-tcp-keep_open">KeepOpen</label>
            <input class="form-check-input" type="checkbox" id="sender-tcp-keep_open" checked>
        </div>
    </div>
</div>